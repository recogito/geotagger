{
  "name": "@recogito/plugin-geotagging",
  "version": "0.1.0",
  "description": "A geo-tagging plugin for Recogito Studio",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 \"src/**/*.css\" \"src/assets/**/*\" dist",
    "dev:watch": "nodemon --watch src --ext ts,tsx,js,jsx,css,json --ignore dist/ --exec 'npm run build'",
    "dev:launch": "npm start --prefix .dev/",
    "dev": "concurrently \"npm run dev:watch\" \"npm run dev:launch\""
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./AdminExtension": "./dist/AdminExtension/index.js",
    "./EditorExtension": "./src/EditorExtension/index.js",
    "./DocumentMapExtension": "./dist/DocumentMapExtension/index.js"
  },
  "dependencies": {
    "@annotorious/react": "^3.3.3",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@recogito/studio-sdk": "^0.3.1",
    "@turf/bbox": "^7.2.0",
    "@turf/centroid": "^7.2.0",
    "cmdk": "^1.1.1",
    "fuse.js": "^7.1.0",
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.4",
    "typesense": "^2.0.3",
    "use-debounce": "^10.0.4",
    "vite-plugin-static-copy": "^2.3.0",
    "wikibase-sdk": "^10.2.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.17",
    "@types/node": "^22.13.15",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "astro": "^5.5.6",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.2"
  }
}
